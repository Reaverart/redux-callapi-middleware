{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
  },
  "rules": {
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "prefer-spread": 2,
    "no-param-reassign": ["error", {"props": false}],
    "no-unused-vars": ["error", { "args": "none" }],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never",
    }],
  }
}
